---
import "../styles/global.css";
// Component Imports
import MainLayout from "@/layouts/main.astro";
import LandingForms from "@/components/homepage/landing-forms";
import { Plus } from "lucide-react";
import { AnimateSvg } from "@/components/homepage/animate";
import { cn } from "@/lib/utils";
import WaitlistButton from "@/components/homepage/waitlistButton";

const pageTitle = "TheFormstore: Create & Design Beautiful Forms";
// Full Astro Component Syntax:
// https://docs.astro.build/basics/astro-components/
