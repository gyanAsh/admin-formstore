<MainLayout title={pageTitle}>
  <main class="relative max-w-full min-h-full overflow-hidden">
    <nav
      class="flex items-center justify-between py-2 px-3 fixed top-0 z-1 w-full bg-zinc-50/20 backdrop-blur-2xl"
    >
      <div class="flex items-center gap-2.5">
        <img src="/formstore-logo-light.svg" class="size-9" />
        <div class="text-lg leading-4 font-semibold">
          <h1 class="font-['Playfair_Display','serif'] italic">The</h1>
          <h1 class="">Formstore</h1>
        </div>
      </div>
      <!-- options -->
      <!-- <div class="bg-zinc-900 text-white rounded-4xl">
        <Button
          className="rounded-4xl backdrop-blur-2xl hover:scale-100 duration-250 transition-colors ease-in-out border-0  "
          variant={"ghost"}
        >
          About</Button
        >
        <Button
          className="rounded-4xl backdrop-blur-2xl hover:scale-100 duration-250 transition-colors ease-in-out border-0  "
          variant={"ghost"}
        >
          Pricing</Button
        >
